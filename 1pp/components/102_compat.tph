PRINT @103016 // ~Copying files...~
SILENT

COPY ~1pp/core/1pp_ext_pal/files/mpalette.bmp~ ~override~
COPY ~1pp/core/1pp_ext_pal/files/mpal256.bmp~ ~override~
COPY ~1pp/core/1pp_ext_pal/files/ranges12.bmp~ ~override~

// END of COMPONENT